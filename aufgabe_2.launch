<launch>
	<include file="$(find turtlebot_stage)/launch/turtlebot_in_stage.launch"/>
	<node name="keyboard" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen" />
	<node name="highlevel_controller" pkg="turtlebot_highlevel_controller" type="turtlebot_highlevel_controller" output="screen">
		<rosparam command="load" file="$(find turtlebot_highlevel_controller)/config/config.yaml" />
	</node>
</launch>
